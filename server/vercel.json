{
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "routes.js"
        }
    ],
    "version": 2,
    "builds": [
        {
            "src": "routes.js",
            "use": "@vercel/node"            
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "index.js",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE",
                "PATCH",
                "OPTIONS"
            ],
            "headers": {
                "Access-Control-Allow-Origin": "https://nexus-tune.vercel.app/",
                "Access-Control-Allow-Credentials": "true",
                "Access-Control-Allow-Headers": "X-CSRF-Token, X-Requested-With, Accept, Accept-Version, Content-Length, Content-MD5, Content-Type, Date, X-Api-Version,Access-Control-Allow-Origin"
            }
        }
    ]
}